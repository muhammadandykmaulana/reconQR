<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.pens.student.mb.bayudwi.jet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ReconTheme">
        <activity android:name=".ReconActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library android:name="com.reconinstruments.os" />
        <uses-library android:name="com.reconinstruments.lib" />

        <activity android:name=".ListItem" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>